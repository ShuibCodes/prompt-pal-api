{
  "kind": "collectionType",
  "collectionName": "app_users",
  "info": {
    "singularName": "app-user",
    "pluralName": "app-users",
    "displayName": "AppUser",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "users-permissions": {
      "config": {
        "jwtSecret": "your-jwt-secret"
      }
    }
  },
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "private": true,
      "minLength": 6
    },
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "submissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::submission.submission",
      "mappedBy": "appUser"
    },
    "result": {
      "type": "string"
    }
  }
}
